<?xml version="1.0"?>
<extension key="de.forumzfd.apiprocessing" type="module">
  <file>apiprocessing</file>
  <name>ForumZFD CiviCRM API Processing</name>
  <description>ForumZFD specific CiviCRM extension for API processing between website(s) and CiviCRM</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Erik Hommel</author>
    <email>hommel@ee-atwork.nl</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/ErikHommel/de.forumzfd.apiprocessing</url>
    <url desc="Documentation">https://github.com/ErikHommel/de.forumzfd.apiprocessing</url>
    <url desc="Support">https://github.com/ErikHommel</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-02-14</releaseDate>
  <version>2.10</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.45</ver>
  </compatibility>
  <requires>
    <ext>de.systopia.xcm</ext>
  </requires>
  <comments>Extension is in production</comments>
  <civix>
    <namespace>CRM/Apiprocessing</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Apiprocessing_Upgrader</upgrader>
</extension>
